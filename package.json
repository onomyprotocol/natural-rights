{
  "name": "natural-rights",
  "version": "0.0.8",
  "description": "",
  "keywords": [],
  "main": "dist/natural-rights.umd.js",
  "module": "dist/natural-rights.es5.js",
  "typings": "dist/types/natural-rights.d.ts",
  "files": ["dist", "es"],
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "lint": "tslint  --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist es",
    "prepare": "npm run prebuild && npm run build",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts && typedoc --out docs --target es6 --theme minimal --mode file src && rimraf es/__tests__",
    "start": "rollup -c rollup.config.ts -w",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "deploy-docs": "ts-node tools/gh-pages-publish",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "precommit": "lint-staged",
    "travis-deploy-once": "travis-deploy-once"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": ["prettier --write", "git add"]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": ["/node_modules/", "/dist/", "/es/"],
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "coveragePathIgnorePatterns": ["/node_modules/", "/test/"],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": ["src/*.{js,ts}", "src/actions/*.{js,ts}"],
    "reporters": [
      "default",
      [
        "./node_modules/jest-html-reporter",
        {
          "pageTitle": "Test Report"
        }
      ]
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@types/express": "^4.17.0",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.7",
    "colors": "^1.3.3",
    "commitizen": "^3.1.1",
    "coveralls": "^3.0.4",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^2.4.0",
    "jest": "^24.8.0",
    "jest-config": "^24.8.0",
    "jest-html-reporter": "^2.5.0",
    "lint-staged": "^8.2.0",
    "lodash.camelcase": "^4.3.0",
    "mkdirp": "^0.5.1",
    "prettier": "^1.18.2",
    "prompt": "^1.0.0",
    "replace-in-file": "^4.1.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.14.4",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.21.1",
    "semantic-release": "^15.13.12",
    "shelljs": "^0.8.3",
    "travis-deploy-once": "^5.0.11",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.2.0",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "@notabug/gun-sear": "^0.0.18",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "isomorphic-fetch": "^2.2.1",
    "node-lmdb": "^0.6.2"
  }
}
